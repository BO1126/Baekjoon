      "source": [
        "Case = int(input())\n",
        "num_list = list(map(int, input().split()))\n",
        "res_counting = 0\n",
        "\n",
        "for i in num_list:\n",
        "    cnt = 0\n",
        "    if(i == 1):\n",
        "        continue\n",
        "    for j in range(2, i + 1):\n",
        "        if(i % j == 0):\n",
        "            cnt += 1\n",
        "    if(cnt == 1):\n",
        "        res_counting += 1\n",
        "print(res_counting)"
      ],
